{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/static/*",
      "/*.{css,scss,js,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot,map}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "401": {
      "statusCode": 302,
      "redirect": "/"
    },
    "403": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "headers": {
        "Cache-Control": "no-cache"
      },
      "rewrite": "/index.html"
    },
    {
      "route": "/index.html",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    }
  ]
}
