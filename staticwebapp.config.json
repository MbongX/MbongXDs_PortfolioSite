{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/static/*", "/*.{css,scss,js,png,jpg,jpeg,gif,ico,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".json": "text/json"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html"
    }
  },
  "routes": [
    {
      "route": "/static/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*",
      "headers": {
        "cache-control": "public, max-age=0, s-maxage=86400, must-revalidate"
      }
    }
  ]
}
